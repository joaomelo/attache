{
  "name": "attache",
  "version": "0.0.1",
  "description": "attache is a serp tracker. it crosses the presence of pages against results of search terms and reports that periodically.",
  "main": "dist/app.js",
  "scripts": {
    "env:firebase": "firebase emulators:start",
    "start": "npm run main:app",
    "test": "npm run test:watch",
    "test:once": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "main:app": "node --enable-source-maps -r dotenv/config dist/app.js",
    "main:cli": "babel-node -r dotenv/config src/main/scale-nedb-cli.js",
    "lint": "eslint --fix --ext .js,.vue src/",
    "build": "rimraf dist/* && npm run build:firebase",
    "build:firebase": "webpack --config webpack.firebase.js",
    "artifacts": "npm run lint && npm run test && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.8.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "inquirer": "^7.3.3",
    "regenerator-runtime": "^0.13.7",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.2",
    "xstate": "^4.15.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.3.0",
    "eslint-webpack-plugin": "^2.4.1",
    "generate-json-webpack-plugin": "^2.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^7.3.0",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-node-externals": "^2.5.2"
  }
}
